              "disable": false,
              "preProcess": {
                "updateDependent": [
                  "populators.options"
                ]
              },
              "populators": {
                "name": "locDetails_ward",
                "optionsKey": "i18nKey",
                "error": "WORKS_REQUIRED_ERR",
                "optionsCustomStyle": {
                  "top": "2.3rem",
                  "maxHeight": "370px"
                },
                "options": []
              }
            },
            {
              "label": "COMMON_LOCALITY",
              "type": "apidropdown",
              "isMandatory": true,
              "disable": false,
              "populators": {
                "name": "locDetails_locality",
                "optionsKey": "i18nKey",
                "allowMultiSelect": false,
                "url": "/egov-location/location/v11/boundarys/_search",
                "params": { "hierarchyTypeCode" : "ADMIN", "boundaryType": "locality" },
                "optionsCustomStyle": {
                  "top": "2.3rem",
                  "maxHeight": "300px"
                },
                "selectFun": "return data?.TenantBoundary[0]?.boundary?.map(item => ({ code: item.code, name: item.name, i18nKey: `${headerLocale}_ADMIN_${item?.code}` }))"
              }
            },
            {

            {
              "label": "MASTERS_BRANCH_NAME",
              "isMandatory": false,
              "key": "financeDetails_branchName",
              "type": "text",
              "disable": true,
              "populators": {
                "name": "financeDetails_branchName",
                "validation": {
                  "minlength": 2
                }
              }
            },
            {
              "type": "component",
              "component": "TransferCodeTable",
              "withoutLabel": true,
              "key": "taxIdentifier",
              "customProps" : {

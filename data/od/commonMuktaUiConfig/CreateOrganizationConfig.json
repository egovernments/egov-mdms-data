            {
              "label": "CORE_COMMON_CITY",
              "isMandatory": true,
              "key": "locDetails_city",
              "type": "radioordropdown",
              "disable": true,
              "preProcess": {
                "updateDependent": [
                  "populators.options"
                ]
              },
              "populators": {
                "name": "locDetails_city",
                "optionsKey": "i18nKey",
                "error": "WORKS_REQUIRED_ERR",
                "optionsCustomStyle": {
                  "top": "2.3rem"
                },
                "options": []
              }
            },
            {
              "label": "COMMON_WARD",
              "isMandatory": true,
              "key": "locDetails_ward",

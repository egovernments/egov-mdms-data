                "convertStringToRegEx": [
                  "populators.validation.pattern"
                ]
              },
              "populators": {
                "name": "locDetails_houseName",
                "error": "WORKS_PATTERN_ERR",
                "validation": {
                  "pattern": "^[a-zA-Z0-9 .,\\/\\-_@#\\']*$",
                  "minlength": 2,
                  "maxlength": 8
                }
              }
            }

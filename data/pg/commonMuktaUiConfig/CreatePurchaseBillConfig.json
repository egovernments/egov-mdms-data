                }
            ]
          },
          {
            "navLink": "PB_Details",
            "head": "",
            "body": [
                {
                    "type": "documentUpload",
                    "withoutLabel": true,
                    "module": "Expenditure",
                    "error": "WORKS_REQUIRED_ERR",
                    "name": "uploadedDocs",
                    "customClass": "",
                    "localePrefix": "EXPENDITURE"
                }
             ]
          }
        ]
      }
  ]
}
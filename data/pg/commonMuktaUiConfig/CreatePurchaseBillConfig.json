                },
                "populators": {
                  "name": "basicDetails_purchaseBillNumber",
                  "customStyle" : {
                    "display" : "none"
                  },
                  "customClass" : "field-value-no-border"
                }
              },
              {
                "label": "WORKS_ORDER_NO",
                "isMandatory": false,
                "key": "basicDetails_workOrderNumber",
                "type": "text",
                "disable": true,
                "populators": {
                  "name": "basicDetails_workOrderNumber",

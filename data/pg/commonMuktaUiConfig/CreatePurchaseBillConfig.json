                "disable": true,
                "populators": {
                  "name": "basicDetails_projectDesc",
                  "customClass": "fc-header-texts"
                }
              },
              {
                "label": "ES_COMMON_LOCATION",
                "isMandatory": false,
                "key": "basicDetails_location",

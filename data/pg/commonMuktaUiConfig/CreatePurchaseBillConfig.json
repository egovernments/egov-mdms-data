                  "name": "basicDetails_location",
                  "customClass": "fc-header-texts"
                }
              }
            ]
          },
          {
            "head": "EXP_INVOICE_DETAILS",
            "subHead": "",
            "body": [
              {
                "key": "invoiceDetails_vendor",
                "label": "EXP_VENDOR",
                "isMandatory": true,
                "type": "dropdown",
                "disable": false,
                "preProcess": {
                  "updateDependent": [
                    "populators.options"
                  ]
                },
                "populators": {
                    "name": "invoiceDetails_vendor",
                    "optionsKey": "name",
                    "error": "WORKS_REQUIRED_ERR",
                    "optionsCustomStyle": {
                        "top": "2.3rem"
                    },
                    "options": []
                }
              },
              {
                "key": "invoiceDetails_vendorId",
                "label": "EXP_VENDOR_ID",
                "isMandatory": true,
                "type": "text",
                "disable": true,
                "populators": {
                  "name": "invoiceDetails_vendorId",
                  "customClass": "disabled-text-field"
                }
              },
              { 
                "key": "invoiceDetails_invoiceNumber",
                "label": "EXP_INVOICE_NUMBER",
                "isMandatory": true,
                "type": "text",
                "disable": false,
                "preProcess": {
                  "convertStringToRegEx": [
                    "populators.validation.pattern"
                  ]
                },
                "populators": {
                  "name": "invoiceDetails_invoiceNumber",
                  "error": "WORKS_REQUIRED_ERR",
                  "validation": {
                    "pattern": "^[a-zA-Z0-9 .\\-_@\\']*$",
                    "maxlength": 64,
                    "minlength": 2
                  }
                }
              },
              {
                "key": "invoiceDetails_invoiceDate",
                "label": "EXP_INVOICE_DATE",
                "isMandatory": true,
                "type": "date",
                "disable": false,
                "preProcess" : {
                  "updateDependent" : ["populators.validation.max"]
                },
                "populators": {
                  "name": "invoiceDetails_invoiceDate",
                  "error": "WORKS_REQUIRED_ERR", 
                  "validation":{
                    "max":"currentDate"
                  }
                }
              },
              {
                "inline": true,
                "label": "EXP_MATERIALCOST_RS",
                "isMandatory": true,
                "key": "invoiceDetails_materialCost",
                "type": "amount",
                "disable": false,
                "preProcess": {
                  "convertStringToRegEx": [
                    "populators.validation.pattern"
                  ]
              },
                "populators": {
                  "name": "invoiceDetails_materialCost",
                  "error": "ERR_INVALID_MAT_COST",
                    "validation": {
                      "pattern": "^[1-9]\\d*(\\.\\d+)?$",
                      "maxlength" : 16,
                      "step" : "0.01",
                      "min" : 0,
                      "max" : 5000000
                    }
                }
              },
              {
                "inline": true,
                "label": "EXP_GST_RS",
                "isMandatory": false,
                "key": "invoiceDetails_gst",
                "type": "amount",
                "disable": false,
                "preProcess": {
                  "convertStringToRegEx": [
                    "populators.validation.pattern"
                  ]
              },
                "populators": {
                  "name": "invoiceDetails_gst",
                  "error": "ERR_INVALID_AMOUNT",
                  "validation": {
                    "pattern": "^[1-9]\\d*(\\.\\d+)?$",
                    "maxlength" : 16,
                    "step" : "0.01",
                    "min" : 0,
                    "max" : 5000000
                  }
                }
              }
            ]
          },
          {
            "navLink":"PB_Details",
            "head": "EXP_BILL_DETAILS",
            "body": [
              {
                "label": "WORKS_BILL_DATE",
                "isMandatory": true,
                "key": "billDetails_billDate",
                "type": "date",
                "disable": true,
                "preProcess" : {
                  "updateDependent" : ["populators.max"]
                },
                "populators": { 
                  "name": "billDetails_billDate",
                  "error": "WORKS_REQUIRED_ERR",
                  "max" : "currentDate"
                }
              },
              {
                "inline": true,
                "label": "EXP_BILL_AMOUNT",
                "isMandatory": false,
                "key": "billDetails_billAmt",
                "type": "text",
                "disable": true,
                "preProcess" : {
                  "updateDependent" : ["populators.customStyle.error"]
                },
                "populators": {
                  "name": "billDetails_billAmt",
                  "customClass": "fc-header-texts",
                  "error": "EXP_BILL_AMOUNT_ERR"
                }
              }
            ]
          },
          {
            "head": "WORKS_DEDUCTIONS",
            "subHead": "",
            "navLink": "PB_Details",
            "body": [
                {
                    "type": "component",
                    "component": "DeductionsTable",
                    "withoutLabel": true,
                    "key": "deductionsDetails"
                }
            ]
          },
          {

{
    "tenantId": "pb",
    "moduleName": "commonSanitationUiConfig",
    "InboxUlbAdminConfig": [
       {
          "label": "ACTION_TEST_TQM_INBOX",
          "type": "inbox",
          "apiDetails": {
             "serviceName": "/inbox/v2/_search",
             "requestParam": {},
             "requestBody": {
                "inbox": {
                   "processSearchCriteria": {
                      "businessService": [
                         "PQM"
                      ],
                      "moduleName": "pqm"
                   },
                   "moduleSearchCriteria": {}
                }
             },
             "minParametersForSearchForm": 0,
             "minParametersForFilterForm": 0,
             "masterName": "commonUiConfig",
             "moduleName": "TqmInboxConfigUlbAdmin",
             "tableFormJsonPath": "requestBody.inbox",
             "filterFormJsonPath": "requestBody.custom",
             "searchFormJsonPath": "requestBody.custom"
          },
          "sections": {
             "search": {
                "uiConfig": {
                   "searchWrapperStyles": {
                      "flexDirection": "column-reverse",
                      "marginTop": "1.4rem",
                      "alignItems": "center",
                      "justifyContent": "end",
                      "gridColumn": "3"
                   },
                   "headerStyle": null,
                   "primaryLabel": "Search",
                   "secondaryLabel": "Clear Search",
                   "minReqFields": 0,
                   "defaultValues": {
                      "id": "",
                      "plantCodes": ""
                   },
                   "fields": [
                      {
                         "label": "TQM_TEST_ID",
                         "type": "text",
                         "isMandatory": false,
                         "disable": false,
                         "populators": {
                            "name": "id",
                            "error": "TQM_ERR_VALID_TEST_ID"
                         }
                      },
                      {
                         "label": "TQM_PLANT_NAME",
                         "type": "dropdown",
                         "isMandatory": false,
                         "disable": false,
                         "populators": {
                            "optionsCustomStyle": {},
                            "name": "plantCodes",
                            "optionsKey": "i18nKey",
                            "allowMultiSelect": false,
                            "mdmsv2": {
                               "schemaCode": "PQM.Plant"
                            }
                         }
                      }
                   ]
                },
                "label": "",
                "children": {},
                "show": true
             },
             "searchResult": {
                "uiConfig": {
                   "columns": [
                      {
                         "label": "TQM_TEST_ID",
                         "jsonPath": "businessObject.id",
                         "additionalCustomization": true
                      },
                      {
                         "label": "TQM_PLANT_NAME",
                         "jsonPath": "businessObject.plantCode",
                         "prefix": "PQM.Plant_",
                         "translate": true
                      },
                      {
                         "label": "TQM_PENDING_DATE",
                         "jsonPath": "businessObject.scheduledDate",
                         "additionalCustomization": true
                      },
                      {
                         "label": "TQM_TREATMENT_PROCESS",
                         "jsonPath": "businessObject.processCode",
                         "prefix": "PQM.Process_",
                         "translate": true
                      },
                      {
                         "label": "TQM_INBOX_STATUS",
                         "jsonPath": "ProcessInstance.state.applicationStatus",
                         "prefix": "WF_STATUS_",
                         "translate": true
                      },
                      {
                         "label": "TQM_INBOX_SLA",
                         "jsonPath": "businessObject.serviceSla",
                         "additionalCustomization": true
                      }
                   ],
                   "enableGlobalSearch": false,
                   "enableColumnSort": true,
                   "resultsJsonPath": "items"
                },
                "children": {},
                "show": true
             },
             "links": {
                "uiConfig": {
                   "links": [
                      {
                         "text": "TQM_VIEW_PAST_RESULTS",
                         "url": "/employee/tqm/search-test-results",
                         "roles": [
                            "PQM_ADMIN"
                         ]
                      },
                      {
                         "text": "TQM_VIEW_IOT_RESULTS",
                         "url": "/employee/tqm/search-test-results",
                         "roles": [
                            "PQM_ADMIN"
                         ]
                      },
                      {
                         "text": "TQM_SENSOR_MON",
                         "url": "/employee/tqm/search-devices",
                         "roles": [
                            "PQM_ADMIN"
                         ]
                      },
                      {
                         "text": "TQM_VIEW_DASHBOARD",
                         "url": "/employee/tqm/dashboard",
                         "roles": [
                            "PQM_ADMIN"
                         ]
                      }
                   ],
                   "label": "TQM_QUALITY_TESTING",
                   "logoIcon": {
                      "component": "TqmInboxIcon",
                      "customClass": "inbox-links-icon"
                   }
                },
                "children": {},
                "show": true
             },
             "filter": {
                "uiConfig": {
                   "formClassName": "filter",
                   "type": "filter",
                   "headerStyle": null,
                   "primaryLabel": "Filter",
                   "minReqFields": 0,
                   "defaultValues": {
                      "processCodes": [],
                      "stage": [],
                      "materialCodes": [],
                      "status": []
                   },
                   "fields": [
                      {
                         "label": "TQM_TREATMENT_PROCESS",
                         "type": "dropdown",
                         "isMandatory": false,
                         "disable": false,
                         "populators": {
                            "allowMultiSelect": true,
                            "name": "processCodes",
                            "optionsKey": "i18nKey",
                            "labelKey": "i18nKey",
                            "masterName": "commonUiConfig",
                            "moduleName": "TqmInboxConfigUlbAdmin",
                            "customfn": "populateMdmsv2SearchReqCriteria",
                            "mdmsv2": {
                               "schemaCode": "PQM.Process"
                            }
                         }
                      },
                      {
                         "label": "TQM_PROCESS_STAGE",
                         "type": "dropdown",
                         "isMandatory": false,
                         "disable": false,
                         "populators": {
                            "name": "stage",
                            "optionsKey": "i18nKey",
                            "allowMultiSelect": true,
                            "mdmsv2": {
                               "schemaCode": "PQM.Stage"
                            }
                         }
                      },
                      {
                         "label": "TQM_OUTPUT_TYPE",
                         "type": "dropdown",
                         "isMandatory": false,
                         "disable": false,
                         "populators": {
                            "allowMultiSelect": true,
                            "name": "materialCodes",
                            "optionsKey": "i18nKey",
                            "masterName": "commonUiConfig",
                            "moduleName": "TqmInboxConfigUlbAdmin",
                            "customfn": "populateMdmsv2SearchReqCriteria",
                            "labelKey": "i18nKey",
                            "mdmsv2": {
                               "schemaCode": "PQM.Material"
                            }
                         }
                      },
                      {
                         "type": "workflowstatesfilter",
                         "isMandatory": false,
                         "disable": false,
                         "populators": {
                            "componentLabel": "TQM_WF_STATUS",
                            "name": "status",
                            "labelPrefix": "WF_STATUS_",
                            "businessService": "PQM"
                         }
                      }
                   ]
                },
                "label": "Filter",
                "show": true
             }
          },
          "additionalSections": {}
       }
    ]
 }